{
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "css.lint.unknownAtRules": "ignore",
    "editor.quickSuggestions": {
        "strings": true
    },
    "tailwindCSS.includeLanguages": {
        "javascript": "javascript",
        "javascriptreact": "javascript",
        "html": "html"
    },
    "tailwindCSS.experimental.classRegex": [
        [
            "className\\s*=\\s*[\"'`]([^\"'`]*)[\"'`]",
            "([^\"'`\\s]*)"
        ]
    ],
    "files.associations": {
        "*.css": "tailwindcss"
    }
}